{
  "expo": {
    "name": "Seasoned",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFF4E0"
      },
      "package": "com.sofiacabral.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen"
    ],
    "splash": {
      "image": "./assets/images/splash.png", 
      "imageWidth": 200,  
      "resizeMode": "contain", 
      "backgroundColor": "#FFF4E0" 
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
